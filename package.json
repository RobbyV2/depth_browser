    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "latest": "^0.2.0",
    "next": "16.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "typescript": "5.8.3"
